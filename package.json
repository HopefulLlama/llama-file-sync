{
  "name": "llama-file-sync",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "eslint": "eslint .eslintrc.js ./src/**/*.js ./test/**/*.js --no-ignore",
    "eslint-fix": "eslint  .eslintrc.js ./src/**/*.js ./test/**/*.js --no-ignore --fix",
    "mocha": "nyc mocha --recursive",
    "test": "npm run eslint && npm run mocha",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    ".eslintrc.js": [
      "./node_modules/.bin/eslint --fix",
      "git add"
    ],
    "src/**/*.js": [
      "./node_modules/.bin/eslint --fix",
      "git add"
    ],
    "test/**/*.js": [
      "./node_modules/.bin/eslint --fix",
      "git add"
    ]
  },
  "author": "Jonathan Law <jonlaw10170@gmail.com> (https://hopefulllama-portfolio.herokuapp.com/)",
  "license": "MIT",
  "dependencies": {
    "chokidar": "^2.0.0",
    "mkdirp": "^0.5.1",
    "rimraf": "^2.6.2",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^4.17.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-prettier": "^2.6.0",
    "husky": "^0.14.3",
    "lint-staged": "^6.1.0",
    "mocha": "^5.0.0",
    "nyc": "^11.4.1",
    "prettier": "^1.10.2",
    "proxyquire": "^1.8.0",
    "sinon": "^4.2.2"
  }
}
